FROM centos:7

RUN yum install -y autoconf automake gcc gcc-c++ libtool libxml2-devel m4 make \
   openssl openssl-devel pkg-config libxslt-devel \
   xsltproc git bc libxml2-python libxslt-python doxygen \
   libpcap-devel bison flex python python-devel python-pip \
   python-setuptools python3 python3-devel python3-pip python3-setuptools \
   rpm-build

CMD ["/usr/bin/cat"]

